      "args": ["mcp", "gateway", "run"]
    },
    "cognee": {
      "type": "sse",
      "url": "http://100.108.215.90:8003/sse"
    },
    "pal": {
      "command": "docker",
      "args": [
        "run",
        "-i",
        "--rm",
        "-v",
        "/Users/jay/projects/pal-mcp-server/conf/custom_models.json:/app/conf/custom_models.json:ro",
        "-v",
        "/Users/jay/projects/pal-mcp-server/.env:/app/.env:ro",
        "-e",
        "ZEN_MCP_FORCE_ENV_OVERRIDE=true",
        "mcp/zen:latest"
      ]
