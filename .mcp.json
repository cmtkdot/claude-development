    "morph-mcp": {
      "command": "npx",
      "args": ["-y", "@morphllm/morphmcp"],
      "env": {
        "MORPH_API_KEY": "sk-8Q9mq6INnTRmD15PHnx9aHHt0lMEO1E6q1QR9Yme4SHTVp0Y",
