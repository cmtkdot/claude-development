{
  "mcpServers": {
    "serena": {
      "type": "stdio",
      "command": "uvx",
      "args": [
        "--from",
        "git+https://github.com/oraios/serena",
        "serena",
        "start-mcp-server",
        "--context",
        "claude-code",
        "--project",
        "xanbzs"
      ],
      "env": {}
    },
    "shadcn": {
      "type": "stdio",
      "command": "npx",
      "args": ["shadcn@latest", "mcp"]
    },
    "expo-docs": {
      "command": "npx",
      "args": ["expo-local-docs-mcp"]
    },
    "ck-search": {
      "type": "stdio",
      "command": "ck",
      "args": ["--serve"],
      "env": {}
    },
    "supabase": {
      "type": "http",
      "url": "https://mcp.supabase.com/mcp?project_ref=swrfsullhirscyxqneay&features=functions%2Cdebugging"
    },
    "tavily": {
      "command": "npx",
      "args": ["-y", "tavily-mcp@latest"],
      "env": {
        "TAVILY_API_KEY": "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"
      }
    },
    "deepwiki": {
      "type": "sse",
      "url": "https://mcp.deepwiki.com/sse"
    },
    "postgresql": {
      "type": "sse",
      "url": "http://100.108.215.90:8006/sse"
    },
    "web-reader": {
      "type": "http",
      "url": "https://api.z.ai/api/mcp/web_reader/mcp",
      "headers": {
        "Authorization": "dcb4a74a31724a0f9d6ffa1e5b36d3ad.8fAkSbTexjReyHjA"
      }
    },
    "zread": {
      "type": "http",
      "url": "https://api.z.ai/api/mcp/zread/mcp",
      "headers": {
        "Authorization": "dcb4a74a31724a0f9d6ffa1e5b36d3ad.8fAkSbTexjReyHjA"
      }
    },
    "ChatPRD": {
      "type": "http",
